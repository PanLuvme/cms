{
    "compilerOptions": {
        "module": "CommonJS",
        "declaration": false,
        "declarationMap": false,
        "sourceMap": false,
        "removeComments": false,
        "strict": true,
        "target": "ES2022",
        "outDir": "./dist-utils/cjs",
        "baseUrl": "./src/_utils",
        "incremental": false,
        "skipLibCheck": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "paths": {
            "@bcms/selfhosted-utils/*": ["./*"]
        }
    },
    "include": ["src/_utils"]
}
